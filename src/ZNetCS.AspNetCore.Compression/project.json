{
  "version": "1.0.3",
  "packOptions": {
    "owners": [ "Marcin Smółka" ],
    "licenseUrl": "https://raw.githubusercontent.com/msmolka/ZNetCS.AspNetCore.Compression/master/LICENSE",
    "projectUrl": "https://github.com/msmolka/ZNetCS.AspNetCore.Compression",
    "repository": {
      "type": "git",
      "url": "https://github.com/msmolka/ZNetCS.AspNetCore.Compression"
    },
    "tags": [
      "aspnetcore",
      "aspnetcoremvc",
      "middleware",
      "compression",
      "decompression",
      "ASP.NET",
      "MVC",
      "gzip",
      "deflate",
      "request",
      "response"
    ]
  },
  "description": "A small package to allow decompress incoming request and compress outgoing response inside ASP.NET Core application.",
  "copyright": "Marcin Smółka zNET Computer Solutions",
  "authors": [ "Marcin Smółka" ],
  "buildOptions": {
    "warningsAsErrors": true,
    "xmlDoc": true,
    "additionalArguments": [ "/ruleset:../../CommonRuleSet.ruleset", "/additionalfile:../../stylecop.json" ]
  },

  "dependencies": {
    "Microsoft.AspNetCore.Http.Abstractions": "1.1.0",
    "Microsoft.Extensions.DependencyInjection.Abstractions": "1.1.0",
    "Microsoft.Extensions.Logging": "1.1.0",
    "Microsoft.Extensions.Options": "1.1.0",
    "Microsoft.Net.Http.Headers": "1.1.0",
    "StyleCop.Analyzers": {
      "version": "1.0.0",
      "type": "build"
    }
  },

  "frameworks": {
    "net451": {
      "dependencies": {
        "System.Diagnostics.Tools": "4.0.0",
        "System.IO.Compression": "4.0.0"
      }
    },
    "netstandard1.6": {
      "dependencies": {
        "System.Diagnostics.Tools": "4.3.0",
        "System.IO.Compression": "4.3.0"
      }
    }
