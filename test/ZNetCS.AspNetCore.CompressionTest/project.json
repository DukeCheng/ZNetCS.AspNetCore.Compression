{
  "version": "1.0.0-*",
  "authors": [ "Marcin Smółka" ],
  "testRunner": "mstest",

  "buildOptions": {
    "warningsAsErrors": false,
    "additionalArguments": [ "/ruleset:../../CommonRuleSet.ruleset", "/additionalfile:../../stylecop.json" ]
  },

  "dependencies": {
    "ZNetCS.AspNetCore.Compression": {
      "version": "*",
      "target": "project"
    },
    "dotnet-test-mstest": "1.1.1-preview",
    "MSTest.TestFramework": "1.0.1-preview",
    "Microsoft.AspNetCore.TestHost": "1.0.0",
    "Microsoft.Net.Http.Headers": "1.0.0",
    "System.IO.Compression": "4.1.1-*",
    "StyleCop.Analyzers": {
      "version": "1.0.0",
      "type": "build"
    }
  },

  "frameworks": {
    "netcoreapp1.0": {
      "imports": [
        "dnxcore50",
        "portable-net45+win8"
      ],

      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.0.1",
          "type": "platform"
        }
      }
    }
  }
}
