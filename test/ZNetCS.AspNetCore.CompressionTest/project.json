{
  "version": "1.0.0-*",
  "authors": [ "Marcin Smółka" ],
  "testRunner": "mstest",

  "buildOptions": {
    "warningsAsErrors": false,
    "additionalArguments": [ "/ruleset:../../CommonRuleSet.ruleset", "/additionalfile:../../stylecop.json" ]
  },

  "dependencies": {
    "Microsoft.NETCore.App": {
      "type": "platform",
      "version": "1.1.0"
    },
    "ZNetCS.AspNetCore.Compression": {
      "version": "*",
      "target": "project"
    },
    "dotnet-test-mstest": "1.1.2-preview",
    "Microsoft.AspNetCore.TestHost": "1.1.0",
    "Microsoft.Net.Http.Headers": "1.1.0",
    "MSTest.TestFramework": "1.1.11",
    "System.IO.Compression": "4.3.0",
    "StyleCop.Analyzers": {
      "version": "1.0.0",
      "type": "build"
    }
  },

  "frameworks": {
    "netcoreapp1.1": {
      "imports": [
        "dnxcore50"        
      ]     
    }
  }
}
